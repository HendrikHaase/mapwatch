{
  "name": "@mapwatch/www",
  "version": "1.0.0",
  "main": "dist/elm.js",
  "license": "GPL-3.0",
  "private": true,
  "scripts": {
    "postinstall": "elm-package install --yes",
    "dev": "rm -rf .dev && cp -rp assets .dev && elm-live src/Main.elm --dir=.dev --output=.dev/elm.js --open",
    "build": "rm -rf dist && cp -rp assets dist && elm-make src/Main.elm --output=dist/elm.js"
  },
  "devDependencies": {
    "elm": "^0.18.0"
  }
}
