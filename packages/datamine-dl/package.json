  "scripts": {
    "fetch:pull": "cd ../.. && git subtree pull --squash --prefix=packages/datamine-dl/steampull https://github.com/erosson/steampull.git master",
    "fetch:install": "pip install -r steampull/requirements-win.txt",
    "fetch:auth": "bash -c \"./steampull/steampull auth\"",
    "fetch": "bash -c \"./steampull/steampull --app 238960 --depot 238961 --output . fetch --regex 'Bundles2\\\\\\\\(Data|Metadata|_\\.index|_Preload|_Startup|_Tiny)'\""
  }
}
